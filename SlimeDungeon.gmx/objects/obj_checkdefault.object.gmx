            <string>// check default build or load dungeon

var isdefault = global._isdefault;


show_debug_message("_isdefault: " + string(isdefault) + "?");

if (isdefault == false){
    return 0;
}

//show_debug_message("_isdefault: " + global._isdefault);

var i;
var j;
for (j = 0; j &lt; global.pathingHeight; j++)
{
    for (i = 0; i &lt; global.pathingWidth; i++)
    {
        mp_grid_add_cell(global.pathing, i, j);
    }
}
var cx = global.pathingWidth div 2;
var cy = global.pathingHeight div 2;

//create dungeon core
instance_create((cx*CELL_WIDTH),(cy*CELL_HEIGHT), obj_dungeon_core);

mp_grid_clear_cell(global.pathing, cx, cy); //center 
mp_grid_clear_cell(global.pathing, cx-1, cy); //left
mp_grid_clear_cell(global.pathing, cx-1, cy-1);//left top
mp_grid_clear_cell(global.pathing, cx, cy-1); //top
mp_grid_clear_cell(global.pathing, cx+1, cy-1);//top right
mp_grid_clear_cell(global.pathing, cx+1, cy);//right
mp_grid_clear_cell(global.pathing, cx+1, cy+1);//bottom right
mp_grid_clear_cell(global.pathing, cx, cy+1);//bottom
mp_grid_clear_cell(global.pathing, cx-1, cy+1);//bottom left
</string>
