            <string>///Sets Alphanurmical characters

//Characters

c[0] = "";

for(var i= 0; i&lt;36; i++)
{
    if(i&lt;10) char = i;
    else char = i+7;
    
    c[i] = chr(48+char);
    show_debug_message(c[i] );
}
//Cursor
horz = 0;
vert = 0;
menu = 0;

