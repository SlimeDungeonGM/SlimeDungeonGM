        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Keyboard

//Draw Characters
draw_set_halign(fa_center);
var line = 0;
var space = 0;
var lineEnd = 11;

for(var i = 0; i &lt; array_length_1d(c); i++)
{
    if(space &gt; lineEnd)
    {
        space = 0;
        line++;
    }
    draw_set_colour(c_white);
    draw_text( (view_wview/2)+(space*25)-128,(view_hview/2)+(line*17)+32,c[i]);
    space++;
}

//Draw 2 extra Buttons
draw_set_colour(c_white);
draw_text((view_wview/2)-30, (view_hview/2), "DEL[D]");
draw_text((view_wview/2)+30, (view_hview/2), "END[C]");

if(keyboard_check_pressed(vk_up)) vert--;
if(keyboard_check_pressed(vk_down)) vert++;
if(keyboard_check_pressed(vk_left)) horz--;
if(keyboard_check_pressed(vk_right)) horz++;
if(horz &lt; 0) horz = 11;
if(horz &gt; 11) horz = 0;
if(vert &lt; 0) vert = 2;
if(vert &gt; 2) vert = 0;
draw_sprite(s_keyboard_curser, 0, (view_wview/2)+(horz*25)-128-8, (view_hview/2)+(vert*17)+32);
// Interact with Menu
if(keyboard_check_pressed(ord('D'))){
    //show_debug_message("D");
    global.username = string_copy(global.username, 0, string_length(global.username)-1);
}

if(keyboard_check_pressed(ord('C'))){
    //show_debug_message("C");
}

if(keyboard_check_pressed(ord('S')))
{
    if(string_length(global.username) &lt; 32){//limited character letters
        global.username += c[horz+(12*vert)];
    }
}

//Draw Name
draw_text((view_wview/2),(view_hview/2)- 32, "NAME:[ "+global.username+ " ]");
//reset align text
draw_set_halign(fa_left);
    
</string>
          </argument>
