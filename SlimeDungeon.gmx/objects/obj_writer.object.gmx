    </event>
    <event eventtype="8" enumb="64">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Keyboard

//Draw Characters
draw_set_halign(fa_center);
var line = 0;
var space = 0;
var lineEnd = 11;

for(var i = 0; i &lt; array_length_1d(c); i++)
{
    if(space &gt; lineEnd)
    {
        space = 0;
        line++;
    }
    draw_set_colour(c_white);
    draw_text(16+(space*25), (view_hview-90)+(line*17),c[i]);
    
    space++;
}

//Draw 2 extra Buttons
draw_set_colour(c_white);
draw_text(view_wview/2+70, view_hview-40, "DEL");
draw_text(view_wview/2+112, view_hview-40, "END");


//Draw Cursor

if(menu == 0)
{
    draw_sprite(s_keyboard_curser, 0, 20+(horz*25), (view_hview-81)+(vert*17));
    
    if(keyboard_check_pressed(vk_left)) horz--;
    if(keyboard_check_pressed(vk_right)) horz++;
    if(horz &lt; 0) horz = 11;
    if(horz &gt; 11) horz = 0;
    
    if(keyboard_check_pressed(vk_up)) vert--;
    if(keyboard_check_pressed(vk_down)) vert++;
   
    if(horz &lt;12 &amp;&amp; horz &gt; 7)
    {
        if(vert &lt; 0){menu = 1; vert = 3; horz = 0;}
        if(vert &lt; 2){menu = 1; vert = 3; horz = 0;}
    }
    else
    {
        if(vert &lt; 0) vert = 2;
        if(vert &gt; 2) vert = 0;
    }
}
else
{
    draw_sprite(s_keyboard_curser, 0, (view_wview/2+82)+(horz*42), view_hview-31);
    
    if(keyboard_check_pressed(vk_left)) horz--;
    if(keyboard_check_pressed(vk_right)) horz++;
    if(horz &lt; 0) horz = 1;
    if(horz &gt; 1) horz = 0;
    
    if(keyboard_check_pressed(vk_up))
    {
        menu = 0;
        vert = 2;
        if(horz == 0) horz = 9;
        if(horz == 1) horz = 11;
    }
    
    if(keyboard_check_pressed(vk_down))
    {
        menu = 0;
        vert = 0;
        if(horz == 0) horz = 9;
        if(horz == 1) horz = 11;
    }
}    

// Interact with Menu
if(keyboard_check_pressed(ord('S')))
{
    if(menu == 0)
    {
        global.username += c[horz+(12*vert)];
    }
    else
    {
        if(horz == 0) global.username = string_copy(global.username, 0, string_length(global.username)+1);
        if(horz == 1)
        {
        
        }
    }
}

//Draw Name
draw_set_halign(fa_center);
draw_text(view_wview/2,view_hview/2.3, global.username);
    
</string>
